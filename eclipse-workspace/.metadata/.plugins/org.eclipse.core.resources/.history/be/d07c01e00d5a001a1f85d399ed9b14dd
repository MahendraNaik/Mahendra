package Calculator;

public class Calculator 
{
	public int addition(int... a)
	{
		int sum=0;
		for(int n:a)
		{
			sum=sum+n;
		}
		return sum;
	}
	public int substraction(int... b)
	{
		int sub=0;
		for(int n:a)
		{
			sub=sub-n;
		}
		return sub;
	}
	public int multiplication(int... c)
	{
		int multi=1;
		for(int n:a)
		{
			multi=multi*n;
		}
		return multi;
	}
	public int division(int... d)
	{
		int div=0;
		for(int n:a)
		{
			div=div/n;
		}
		return div;
	}

}
