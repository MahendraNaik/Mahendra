package D8Threads;

class Bank implements Runnable
{
	

	@Override
	public void run() {
		for(int i=0;i<1;i++)
		{
			count++;
		}
		
	}

}
public class MainBank
{
	static int count;
	public static void main(String[] args) 
	{
		Thread t1 = new Thread(new Bank());
		Thread t2 = new Thread(new Bank();
	}
}
