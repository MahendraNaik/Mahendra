package D8Threads;

class Bank implements Runnable
{
	static int count;

	@Override
	public void run() {
		for(int i=0;i<100;i++)
		{
			count++;
			//System.out.println(count);
		}
		
	}

}
public class MainBank
{
	
	public static void main(String[] args) 
	{
		Bank.count=1;
		Thread t1 = new Thread(new Bank());
		Thread t2 = new Thread(new Bank());
		
		t1.start();
		t2.start();
		System.out.println(Bank.count);
		
	}
}
