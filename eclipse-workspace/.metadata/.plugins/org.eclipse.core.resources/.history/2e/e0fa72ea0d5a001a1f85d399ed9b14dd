package Calculator;

public class Calculator 
{
	public int addition(int... a)
	{
		int sum=0;
		for(int n1:a)
		{
			sum=sum+n1;
		}
		return sum;
	}
	public int substraction(int... b)
	{
		int sub=0;
		for(int n2:a)
		{
			sub=sub-n2;
		}
		return sub;
	}
	public int multiplication(int... c)
	{
		int multi=1;
		for(int n3:a)
		{
			multi=multi*n3;
		}
		return multi;
	}
	public int division(int... d)
	{
		int div=0;
		for(int n4:a)
		{
			div=div/n4;
		}
		return div;
	}

}
