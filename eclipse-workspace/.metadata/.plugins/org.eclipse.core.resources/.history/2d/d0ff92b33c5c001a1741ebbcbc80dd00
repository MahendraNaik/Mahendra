package CollageApplication;

public class Student 
{
	private String rollNum;
	private String name;
	private String gender;
	Subject subject;
	private int grade;
	private int marks;
	
	public Student(String rollNum, String name, String gender, Subject subject, int grade, int marks) 
	{
		this.rollNum = rollNum;
		this.name = name;
		this.gender = gender;
		this.subject = subject;
		this.grade = grade;
		this.marks = marks;
	}

	public String getRollNum()
	{
		return rollNum;
	}

	public void setRollNum(String rollNum) 
	{
		this.rollNum = rollNum;
	}

	public String getName() 
	{
		return name;
	}

	public void setName(String name) 
	{
		this.name = name;
	}

	public String getGender() 
	{
		return gender;
	}

	public void setGender(String gender) 
	{
		this.gender = gender;
	}

	public Subject getSubject() 
	{
		return subject;
	}

	public void setSubject(Subject subject) 
	{
		this.subject = subject;
	}

	public int getGrade() 
	{
		return grade;
	}

	public void setGrade(int grade) 
	{
		this.grade = grade;
	}

	public int getMarks() 
	{
		return marks;
	}

	public void setMarks(int marks) 
	{
		this.marks = marks;
	}

	@Override
	public String toString() 
	{
		return "Student RollNum=" + rollNum + ", name=" + name + ", gender=" + gender + ", subject=" + subject
				+ ", grade=" + grade + ", marks=" + marks ;
	}
	
	
}
