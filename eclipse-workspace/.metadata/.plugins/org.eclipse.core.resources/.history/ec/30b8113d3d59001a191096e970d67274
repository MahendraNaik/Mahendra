package D8Threads.D8PlaceOrder;

class PlaceOrder implements Runnable
{
	static int qt = 10;
	@Override
	public void run() 
	{
		orderPlace(6);
	}
	
	public void orderPlace(int q)
	{
		if(q<qt)
		{
			System.out.println(" Order Succesfull");
		}
	}
	
}

public class Order 
{
	public static void main(String[] args) 
	{
		
		
	}
}
