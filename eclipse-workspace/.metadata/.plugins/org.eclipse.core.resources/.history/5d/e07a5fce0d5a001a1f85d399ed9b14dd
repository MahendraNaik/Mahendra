package Calculator;

public class Calculator 
{
	public int addition(int... a)
	{
		int sum=0;
		for(int n:a)
		{
			sum=sum+n;
		}
		return sum;
	}
	public int substraction(int... b)
	{
		int sum=0;
		for(int n:a)
		{
			sum=sum+n;
		}
		return sum;
	}
	public int multiplication(int... c)
	{
		int sum=0;
		for(int n:a)
		{
			sum=sum+n;
		}
		return sum;
	}
	public int division(int... d)
	{
		int sum=0;
		for(int n:a)
		{
			sum=sum+n;
		}
		return sum;
	}

}
